<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
   	    <shape xmlns:android="http://schemas.android.com/apk/res/android">
			<gradient
				android:type="linear"
				android:startColor="#68A6E6"
				android:endColor="#116696"
				android:angle="90"/>
		</shape> 
    </item>
    <item android:state_focused="true" >
   	    <shape xmlns:android="http://schemas.android.com/apk/res/android">
			<gradient
				android:type="linear"
				android:startColor="#68A6E6"
				android:endColor="#116696"
				android:angle="90"/>
		</shape> 
    </item>
    <item >
	    <shape xmlns:android="http://schemas.android.com/apk/res/android">
			<gradient
				android:type="linear"
				android:startColor="#99D6E6"
				android:endColor="#C8FAFF"
				android:angle="90"/>
		</shape> 
	</item>
</selector>